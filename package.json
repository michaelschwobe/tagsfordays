{
  "name": "tagsfordays",
  "description": "TagsForDays extends traditional bookmarking with advanced organization and search capabilities.",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Michael Schwobe <mschwobe@gmail.com> (https://schwobe.dev)",
  "repository": "github:michaelschwobe/tagsfordays",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "remix build",
    "clean": "rm -rf .cache && rm -rf coverage && rm -rf build && rm -rf public/build && rm -rf node_modules",
    "dev": "remix dev --manual",
    "format": "prettier --ignore-unknown --write .",
    "lint": "eslint --ext=.js,.jsx,.ts,.tsx .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve ./build/index.js",
    "studio": "prisma studio",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@conform-to/react": "^0.9.0",
    "@conform-to/zod": "^0.9.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.3.1",
    "@radix-ui/react-select": "^1.2.2",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cookie": "^0.5.0",
    "isbot": "^3.7.0",
    "prisma": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.0.3",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/bcryptjs": "^2.4.4",
    "@types/cookie": "^0.5.2",
    "@types/node": "^20.7.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.7.0",
    "pnpm": ">=8.7.6"
  }
}
