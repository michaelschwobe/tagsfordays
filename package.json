{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "remix build",
    "clean": "rm -rf .cache && rm -rf coverage && rm -rf build && rm -rf public/build && rm -rf node_modules",
    "dev": "remix dev",
    "format": "prettier --ignore-unknown --write .",
    "lint": "eslint --ext=.js,.jsx,.ts,.tsx .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "studio": "prisma studio",
    "typecheck": "tsc",
    "reset": "pnpm run clean && pnpm install && pnpm dlx prisma migrate dev --name init && pnpm run setup"
  },
  "dependencies": {
    "@conform-to/react": "^0.8.0",
    "@conform-to/zod": "^0.8.0",
    "@prisma/client": "^5.1.1",
